<script src="http://code.jquery.com/jquery.js"></script>
<script>
	$(document).ready(function() {
		$('.outer').mouseover(function() {
			$('body').append('<h1>MOUSEOVER</h1>');
		}).mouseenter(function() {
			$('body').append('<h1>MOUSENTER</h1>');
		});
	});
</script>
</head>
